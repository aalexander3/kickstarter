<h1>Current Projects</h1>
<% @projects.each do |project| %>
<p><%= link_to project.name, project_path(project), method: 'get' %></p>
<p><%= project.description %></p>
<p><%= project.goal %></p>
<p><%= project.contributions %></p>
<% end %><br>

<%= button_to 'Create New Project', new_project_path, method: "get" %><br>
